# MotorsportsReg OAuth 1.0a Credentials
# Copy this file to .env and fill in your actual credentials
# NEVER commit .env to source control

# Consumer credentials (from MotorsportsReg)
MSR_CONSUMER_KEY=your_consumer_key_here
MSR_CONSUMER_SECRET=your_consumer_secret_here

# API Configuration
MSR_BASE_URL=https://api.motorsportreg.com

# OAuth callback (leave blank to use localhost callback server)
# MSR_CALLBACK_URL=
MSR_CALLBACK_PORT=8089

# Organization ID (populated after first authentication)
# You can find this in the /rest/me response after authenticating
MSR_ORGANIZATION_ID=
